# Containers Stak
STACK_NAME="adempiere-s3-gateway-service"

## Generic Restart
GENERIC_RESTART="always"

# S3
S3_IMAGE="quay.io/minio/minio"
S3_HOST="${STACK_NAME}.s3"
S3_PORT=9000
S3_CONSOLE_PORT=9090
S3_USER=adempiere
S3_PASSWORD=adempiere
S3_VOLUME="${STACK_NAME}.volume_s3"

S3_GATEWAY_RS_HOST="default-stack.s3.gateway.rs"
S3_GATEWAY_RS_IMAGE="openls/s3-gateway-rs:1.0.3"
S3_GATEWAY_RS_PORT=7878
# Values
S3_GATEWAY_RS_S3_URL="http://localhost:9000"
S3_GATEWAY_RS_BUCKET_NAME="test" # Fill It
S3_GATEWAY_RS_API_KEY="adempiere" # Fill It
S3_GATEWAY_RS_SECRET_KEY="adempiere" # Fill It

# Nginx Gateway
GATEWAY_IMAGE="nginx:1.25"
GATEWAY_HOST="${STACK_NAME}.ui.gateway"
GATEWAY_VOLUME="${STACK_NAME}.volume_nginx"
GATEWAY_PORT=80

# Networks
DEFAULT_NETWORK="default-stack.adempiere_network"